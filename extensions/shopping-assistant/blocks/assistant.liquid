<div id="edna-assistant-container"></div>

{% schema %}
{
  "name": "Edna Shopping Assistant",
  "target": "body",
  "stylesheet": "edna-styles.css",
  "javascript": "edna-assistant.js",
  "settings": [
    {
      "type": "color",
      "id": "primary_color",
      "label": "Primary Color",
      "default": "#2ECC71"
    },
    {
      "type": "color",
      "id": "secondary_color",
      "label": "Secondary Color",
      "default": "#FFFFFF"
    }
  ]
}
{% endschema %}